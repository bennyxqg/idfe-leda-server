<script src="js/jquery-1.8.2.min.js" type="text/javascript"></script>
<script>
    var survey_list = new Array(28);
    var isfirst = new Array(9);
    var qid = "question_1";
    var score = 0;
    var inputs = document.getElementsByTagName("input");
    for (var i = 0; i < inputs.length; i++) {
        if (inputs[i].type == 'button' && inputs[i].value == "下一页") {
            inputs[i].addEventListener("click", Test, false);
        }
    }
    for (var i = 0; i < inputs.length; i++) {
        if (inputs[i].type == 'button' && inputs[i].value == "上一页") {
            inputs[i].addEventListener("click", LastPage, false);
        }
    }
    for (var i = 0; i < inputs.length; i++) {
        if (inputs[i].type == 'radio') {
            inputs[i].addEventListener("click", turnpa, false);
        }
    }
    function turnpa() {
        var laststring = qid.substring(qid.length - 1);
        if (isfirst[laststring] == "1") return;
        tf(1);

    }
    function LastPage() {
        var laststring = qid.substring(qid.length - 1);
        var a = qid.substring(0, qid.length - 1);
        document.getElementById(a + laststring).style.display = "none";
        document.getElementById((a + (Number(laststring) - 1).toString())).style.display = "block";
        qid = a + (Number(laststring) - 1).toString();
    }
    function Test() {
        tf(0);
    }

    function tf(e) {
        var rname = '';
        var flag = true;
        var inps = document.getElementById(qid).getElementsByTagName("input");
        for (var i = 0; i < inps.length; i++) {
            if (inps[i].type == 'radio') {
                var thisname = inps[i].name;
                if (thisname != rname) {
                    if (!flag) {
                        if (e == 0)
                            alert("请完成所有题目后进入下一页！谢谢！")
                        break;
                    }
                    flag = false;
                    rname = thisname;
                }
            }

            if (inps[i].type == 'radio' && inps[i].name == rname && inps[i].checked) {
                flag = true; //有被选中
            }

            if (i == inps.length - 1) {
                if (flag == true) {
                    var chosco = 0;
                    for (var i = 0; i < inps.length; i++) {
                        if (inps[i].type == 'radio' && inps[i].checked) {
                            if (qid == "question_8")
                                chosco = Number(inps[i].value);
                        }
                    }
                    var laststring = qid.substring(qid.length - 1);
                    var a = qid.substring(0, qid.length - 1);
                    isfirst[laststring] = "1";
                    document.getElementById(a + laststring).style.display = "none";
                    document.getElementById((a + (Number(laststring) + 1).toString())).style.display = "block";
                    qid = a + (Number(laststring) + 1).toString();
                    if (laststring == 8) {
                        var inp = document.getElementsByTagName("input");
                        for (var i = 0; i < inp.length; i++) {
                            if (inp[i].type == "radio" && inp[i].checked) {
                                var radname = inp[i].name;
                                var key = (Number(radname.substring(9, radname.length)) - 1).toString();
                                score += Number(inp[i].value);
                                survey_list[key] = inp[i].value;
                            }
                        }
                        document.getElementById("lbl_score").innerHTML = score.toString() + "分";
                        score = 0;
                        if (chosco > 2) {
                            document.getElementById("score1").style.display = "block";
                            document.getElementById("score2").style.display = "none";
                        } else {
                            document.getElementById("score1").style.display = "none";
                            document.getElementById("score2").style.display = "block";
                        }
                    }
                }
            }
        }
    }

    wk = function (e) {
        var _keyCode = e.keyCode;
        if (e.shiftKey) { return false; } //禁用shift键，防止输入其他字符
        if ((_keyCode > 47 && _keyCode < 58) || (_keyCode > 95 && _keyCode < 106) || (_keyCode > 36 && _keyCode < 41) || (_keyCode == 46) || (_keyCode == 8) || (_keyCode == 9)) {
            return true;
        }
        return false;
    };

    $(function () {
        $("input[name=phone]").keydown(function (e) {
            return wk(e);
        });
        $("#get_code").click(function () {
            var phone = $("input[name=phone]").val();
            if (phone == "") {
                alert("请输入您的手机号码");
                return false;
            }
            $.ajax({
                type: "POST",
                url: "/survey",
                data: { phone: phone,
                    survey_list: survey_list
                },
                dataType: "json",
                success: function (respon) {
                    alert(respon.msg);
                }
            });
        });
    });
</script>
