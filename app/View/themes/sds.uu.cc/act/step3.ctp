function getQueryString(name) {
var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
var r = window.location.search.substr(1).match(reg);
if (r != null) return unescape(r[2]); return null;
}

//提交、输出分数
function Result() {
	score = 0;
	$("#questions .pro").each(function(){
		var _scoreNum = parseInt($(this).attr("data-num"));
		score += _scoreNum;
	});
	var phone=getQueryString("phone");
	$.ajax({
		type:"POST",
		url:"//sds.uu.cc/act/3",
		data:{score:score,phone:phone},
		dataType:"json",
		success:function(res){
			if(res.ret == 2){
				alert("您已经参加过报名！");
			}else if(res.ret == 0){
				var link_go = res.share_url;
				window.location=link_go;
			}else{
			   alert("提交失败！");
			}
		}
	});
}

var intDiff = parseInt(1800);//倒计时总秒数量
function timer(intDiff){
	window.setInterval(function(){
	var minute=0,
		second=0;//时间默认值		
	if(intDiff > 0){
		minute = Math.floor(intDiff / 60);
		second = Math.floor(intDiff) - (minute * 60);
	}
	if (minute <= 9) minute = '0' + minute;
	if (second <= 9) second = '0' + second;

	$('#minute_show').html(minute);
	$('#second_show').html(second);
	intDiff--;
	if(intDiff == 0){
		$('.test_btn').removeAttr('onclick');
		Result();
	}
	}, 1000);
} 
