<script type="text/javascript">
$(function(){
	$('.yuyue').click(function(){
		$('.zhezao').show();
		$('#shuru').attr('value','请输入手机号');
		$('.tk').show();
		
		})
	$('.zhezao, .close').click(function(){
		$('.zhezao').hide();
		$('.tk').hide();
		$('#shuru').attr('value','请输入手机号');
		})
		
	//手机号预约事件
	var type = 1;
	var myreg = /^1[34578]\d{9}$/;
		$('#shuru').focus(function(){
		if($(this).attr('value') == '请输入手机号'){}
		  $(this).attr('value','');
		})
		
	$('#shuru').blur(function(){
	   if($(this).attr('value') == ''){
		$(this).attr('value','请输入手机号');
		}
		})	
	
    $('.yu_yue').click(function(){
		var phone = $('#shuru').val();
		if(phone == '请输入手机号' || phone == ''){
			alert('请输入手机号哦！');
			}else{
			if(!myreg.exec(phone)){
				alert('您输入的手机号码不对哦！');
				}else{
				var type = $('input:radio[name="banben"]:checked').val();
				/*
				if($('.az').is('.on')){
					type = 2;
					}else{
					type = 1;
				}
				*/
				var collect_name;
				// alert(type + "---" + phone + "collect_name");
				 $.ajax({
					type:"POST",
					url:"/collect_phone",
					data:{phone:phone,type:type,collect_name:77},
					dataType:"json",
					success:function(res){
						if(res.ret == 2){
						
							alert("您好，您已成功预约，无需重复操作。");
						}else if(res.ret == 0){
							 alert("恭喜，您已成功预约《J Game》超前体验。");
						}else{
						   alert("您好，您本次预约未成功，请重新尝试。");
						}
					}
				});

			}	
		}
	    })
})
