        }

        /* - 以上为reset样式,页面样式这里起  - */
        .ghp-game-box, .ghp-game-inner, .lnk, .name, .ico {
            background: url("/css/gamelist/ghp_g_bg.png") no-repeat;
        }

        .ghp-game-box {
            padding-top: 11px;
            width: 813px;
            background-position: -813px -489px;
        }

        .ghp-game-inner {
            background-position: 0 bottom;
            overflow: hidden;
            padding: 0 7px 7px;
            line-height: 1.5;
        }

        .lst {
            width: 111px;
            float: left;
            margin: 0 -1px 0 0;
        }

        .mmo {
            width: 111px;
        }

        .etc {
            width: 333px;
        }

        .ath {
            width: 220px;
        }

        .name {
            height: 34px;
            line-height: 34px;
            background-position: -813px -68px;
            font-size: 12px;
            color: #0096D0;
            text-indent: 10px;
        }

        .mmo .name {
            background-position: -813px 0;
        }

        .etc .name {
            background-position: -813px 0px;
        }

        .ath .name {
            background-position: -813px -34px;
        }

        .amount {
            font-size: 9px;
            color: #BBBFCB;
            padding: 0 5px;
        }

        .lnk {
            display: block;
            width: 101px;
            height: 28px;
            padding: 0 10px 0 10px;
            line-height: 28px;
            background-position: -813px -102px;
            color: #464646;
            text-decoration: none;
        }

        .lnk:hover {
            color: #0896D4;
        }

        .ico {
            display: block;
            width: 10px;
            height: 28px;
            overflow: hidden;
            text-indent: 500px;
            float: right;
            margin-right: 10px;
        }

        .hot {
            background-position: -813px -192px;
        }

        .beta {
            background-position: -813px -232px;
        }

        .new {
            background-position: -813px -272px;
        }

        .more {
            background-position: -813px -309px;
            float: left;
        }

        .ghp-game-box1 {
